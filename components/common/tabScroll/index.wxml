<scroll-view scroll-x scroll-with-animation scroll-left="{{ scrollLeft }}" class="tab-scroll">
  <view class="tab-title">
    <view class="tab-item{{ currentIndex===index?' tab-active':'' }}" wx:for="{{ list }}" wx:key="index"
      data-index="{{ index }}" bindtap="onTabItem">{{ nameKey?item[nameKey]:item }}</view>
  </view>
</scroll-view>
